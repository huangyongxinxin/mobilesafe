<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <TextView
        style="@style/AppTitle"
        android:text="设置中心"
        />
    <com.txs.mobilesafe.activity.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.txs.mobilesafe"
        android:id="@+id/setting_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        mobilesafe:title="自动更新设置"
        mobilesafe:desoff="自动更新已开启"
        mobilesafe:deson="自动更新已关闭"

        >
    </com.txs.mobilesafe.activity.SettingItemView>

    <com.txs.mobilesafe.activity.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.txs.mobilesafe"
        android:id="@+id/setting_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        mobilesafe:title="电话归属地设置"
        mobilesafe:desoff="归属地的显示已开启"
        mobilesafe:deson="归属地的显示已关闭"
        >
    </com.txs.mobilesafe.activity.SettingItemView>

    <com.txs.mobilesafe.view.SettingClickView
        android:id="@+id/scv_toast_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.txs.mobilesafe.view.SettingClickView>

    <com.txs.mobilesafe.view.SettingClickView
        android:id="@+id/scv_activity_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.txs.mobilesafe.view.SettingClickView>


    <com.txs.mobilesafe.activity.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.txs.mobilesafe"
        android:id="@+id/setting_blacknum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        mobilesafe:title="黑名单拦截设置"
        mobilesafe:desoff="黑名单拦截已开启"
        mobilesafe:deson="黑名单拦截已关闭">
    </com.txs.mobilesafe.activity.SettingItemView>
